{
  "app_info": {
    "name": "杭種コード自動追加アプリ",
    "version": "1.0.0",
    "description": "AI画像認識による杭種判定・コード自動追加システム"
  },
  "pile_codes": {
    "plastic": "P",
    "plate": "PL", 
    "metal": "B",
    "concrete": "C",
    "target": "T",
    "kokudo": "KD",
    "gaiku_kijun": "GK",
    "gaiku_takaku": "GT",
    "gaiku_hojo": "GH",
    "gaiku_setsu": "GS"
  },
  "model_settings": {
    "model_path": "models/pile_classifier.h5",
    "confidence_threshold": 0.7,
    "image_size": [224, 224],
    "batch_size": 128
  },
  "training_settings": {
    "image_size": [320, 320],
    "batch_size": 128,
    "epochs": 30,
    "learning_rate": 0.001,
    "memory_optimization": true,
    "class_order": [
      "plastic",
      "plate", 
      "byou",
      "concrete",
      "traverse",
      "kokudo",
      "gaiku_sankaku",
      "gaiku_setsu",
      "gaiku_takaku",
      "gaiku_hojo",
      "traverse_in",
      "kagoshima_in"
    ],
    "required_classes": [
      "plastic",
      "plate", 
      "byou",
      "concrete",
      "traverse",
      "kokudo",
      "gaiku_sankaku",
      "gaiku_setsu",
      "gaiku_takaku",
      "gaiku_hojo",
      "traverse_in",
      "kagoshima_in"
    ]
  },
  "processing_settings": {
    "supported_extensions": [".jpg", ".jpeg", ".png", ".bmp", ".tiff"],
    "backup_enabled": true,
    "manual_confirmation_threshold": 0.9,
    "max_file_size_mb": 30
  },
  "ui_settings": {
    "window_size": "800x600",
    "theme": "default",
    "log_max_lines": 1000
  }
}
